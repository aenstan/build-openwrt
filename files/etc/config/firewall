
config defaults
	option input 'ACCEPT'
	option output 'ACCEPT'
	option flow_offloading '0'
	option flow_offloading_hw '0'
	option fullcone '2'
	option forward 'REJECT'
	option syn_flood '1'

config zone
	option name 'lan'
	list network 'lan'
	option input 'ACCEPT'
	option output 'ACCEPT'
	option forward 'ACCEPT'
	option masq '1'
	option mtu_fix '1'

