
config interface 'loopback'
	option ifname 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config interface 'lan'
	option proto 'static'
	option netmask '255.255.255.0'
	option delegate '0'
	option _orig_ifname 'eth0'
	option _orig_bridge 'true'
	option ipaddr '10.0.0.1'
	option ifname 'eth0'
	option gateway '10.0.0.3'
	option dns '10.0.0.1'

config device
	option type 'bridge'
	option name 'docker0'

config interface 'utun'
	option proto 'none'
	option ifname 'utun'
	option device 'utun'

